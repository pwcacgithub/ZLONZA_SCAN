       <REFERENCE>X</REFERENCE>
       <TYP>XFELD</TYP>
      </RSIMP>
     </IMPORT>
     <EXPORT>
      <RSEXP>
       <PARAMETER>EV_CONFIRM_RESULT</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>CHAR01</TYP>
      </RSEXP>
     </EXPORT>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>IV_LINE_SIZE</PARAMETER>
       <KIND>P</KIND>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IV_LINES</PARAMETER>
       <KIND>P</KIND>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IV_FOR_CONFIRM</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Checkbox</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>EV_CONFIRM_RESULT</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Y&apos; = Yes   ;  &apos;N&apos; = No</STEXT>
      </RSFDO>
     </DOCUMENTATION>
    </item>
