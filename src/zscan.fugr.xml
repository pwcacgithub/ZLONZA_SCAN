     <SHORT_TEXT>Display Messge in Scanner</SHORT_TEXT>
     <IMPORT>
      <RSIMP>
       <PARAMETER>IV_LINE_SIZE</PARAMETER>
       <DEFAULT>40</DEFAULT>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>I</TYP>
      </RSIMP>
